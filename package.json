{
  "name": "@adddog/eslint",
  "version": "0.0.13",
  "type": "module",
  "homepage": "https://github.com/samelie/eslint#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/samelie/eslint.git"
  },
  "bugs": {
    "url": "https://github.com/samelie/eslint/issues"
  },
  "exports": {
    ".": {
      "import": "./src/eslint.config.ts"
    }
  },
  "main": "./src/eslint.config.ts",
  "types": "./src/eslint.config.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "prepublishOnly": "pnpm run types && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "knip": "knip",
    "version:patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@adddog/eslint-plugin-rules": "workspace:*",
    "@antfu/eslint-config": "^7.4.3",
    "eslint-plugin-no-barrel-files": "^1.2.2"
  },
  "devDependencies": {
    "@adddog/monorepo-consistency": "workspace:*",
    "@types/node": "^25.3.0",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.2",
    "eslint-plugin-format": "^1.4.0",
    "package-json-type": "^1.1.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "exports": {
      ".": {
        "types": "./dist/eslint.config.d.ts",
        "import": "./dist/eslint.config.mjs",
        "require": "./dist/eslint.config.cjs"
      }
    },
    "main": "./dist/eslint.config.mjs",
    "types": "./dist/eslint.config.d.ts"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  }
}
